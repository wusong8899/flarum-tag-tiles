#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/code/lg/now/ziiven/flarum-tag-tiles/js/node_modules/.pnpm/vite@7.1.2_@types+node@24.3.0_terser@5.43.1/node_modules/vite/bin/node_modules:/mnt/d/code/lg/now/ziiven/flarum-tag-tiles/js/node_modules/.pnpm/vite@7.1.2_@types+node@24.3.0_terser@5.43.1/node_modules/vite/node_modules:/mnt/d/code/lg/now/ziiven/flarum-tag-tiles/js/node_modules/.pnpm/vite@7.1.2_@types+node@24.3.0_terser@5.43.1/node_modules:/mnt/d/code/lg/now/ziiven/flarum-tag-tiles/js/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/code/lg/now/ziiven/flarum-tag-tiles/js/node_modules/.pnpm/vite@7.1.2_@types+node@24.3.0_terser@5.43.1/node_modules/vite/bin/node_modules:/mnt/d/code/lg/now/ziiven/flarum-tag-tiles/js/node_modules/.pnpm/vite@7.1.2_@types+node@24.3.0_terser@5.43.1/node_modules/vite/node_modules:/mnt/d/code/lg/now/ziiven/flarum-tag-tiles/js/node_modules/.pnpm/vite@7.1.2_@types+node@24.3.0_terser@5.43.1/node_modules:/mnt/d/code/lg/now/ziiven/flarum-tag-tiles/js/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
